    "axios": "^0.18.0",
    "axios-extensions": "^3.0.5",
    "classnames": "^2.2.6",
    "dexie": "^2.0.4",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "i18next": "^12.1.0",
    "i18next-browser-languagedetector": "^2.2.4",
    "i18next-xhr-backend": "^1.5.1",
    "jsdom": "^13.0.0",
