{
	"name": "webapp",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"ajv": "^6.5.3",
		"axios": "^0.18.0",
		"babel-core": "^6.26.0",
		"history": "^4.7.2",
		"immutable": "^3.8.2",
		"react": "^16.4.2",
		"react-dom": "^16.4.2",
		"react-redux": "^5.0.7",
		"react-router-dom": "^4.3.1",
		"react-scripts": "^2.1.1",
		"redux": "^4.0.0",
		"redux-form": "^7.4.2",
		"redux-logger": "^3.0.6",
		"redux-saga": "^0.16.0",
		"reselect": "^3.0.1",
		"semantic-ui-css": "^2.3.3",
		"semantic-ui-react": "^0.82.4",
		"styled-components": "^3.4.6"
	},
	"scripts": {
		"analyze": "source-map-explorer build/static/js/main.*",
		"build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
		"watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
		"start-js": "react-scripts start",
		"start": "npm-run-all -p watch-css start-js",
		"build-js": "react-scripts build",
		"build": "npm-run-all build-css build-js && yarn run delete-maps",
		"delete-maps": "yarn run delete-map-files && yarn run delete-references-to-map-files",
		"delete-map-files": "find ./build -name '*.map' -delete",
		"delete-references-to-map-files": "find ./build -regex '.*\\.\\(js\\|css\\)' -exec sed -i -E '\\/[\\*\\/]#\\ssourceMappingURL=main(\\.[0-9a-f]+)?\\.(css|js)\\.map(\\*\\/)?/g' {} +",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject"
	},
	"devDependencies": {
		"babel-eslint": "^9.0.0",
		"eslint": "^5.3.0",
		"eslint-config-airbnb": "17.1.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-jsx-a11y": "^6.1.1",
		"eslint-plugin-prettier": "^2.6.2",
		"eslint-plugin-react": "^7.11.0",
		"node-sass-chokidar": "^1.3.3",
		"npm-run-all": "^4.1.3",
		"prettier": "^1.14.2",
		"source-map-explorer": "^1.6.0",
		"redux-devtools-extension": "^2.13.5"
	}
}
